{
    "name": "jonston/symfony-permission",
    "description": "A basic permission management bundle for Symfony, similar to spatie/laravel-permission",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonston",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.0|^7.0",
        "doctrine/orm": "^2.10|^3.0",
        "doctrine/doctrine-bundle": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Jonston\\SymfonyPermission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonston\\SymfonyPermission\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
